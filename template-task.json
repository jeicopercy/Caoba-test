{
    "family": "%COMPONENTE%",
    "containerDefinitions": [
        {
            "image": "891899566293.dkr.ecr.us-east-2.amazonaws.com/%AMBIENTE%/%COMPONENTE%:%TAGBUILDECS%",
            "name": "%COMPONENTE%",
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/%COMPONENTE%-task",
                    "awslogs-region": "%REGION%",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "tags": [
        {
          "key": "version",
          "value": "%TAGBUILDECS%"
        }
      ]
}
